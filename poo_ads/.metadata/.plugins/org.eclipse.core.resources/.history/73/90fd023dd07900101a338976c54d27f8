package aula2;

public class Principal {
	
	public static void main(String[] args) {
		
		Carro c1 = new Carro();
		c1.modelo = "Civic";
		c1.marca = "Honda";
		c1.preco = 12000;
		c1.potencia = 184;
		
		Carro c2 = new Carro();
		c2.modelo = "Argo";
		c2.marca = "Fiat";
		c2.preco = 90000;
		c2.potencia = 80;
		
		c2.acelerar();
		
	}

}
